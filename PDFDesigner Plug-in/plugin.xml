      <wizard
            name="PDF Designer File"
            icon="icons/sample.gif"
            category="fr.malaaria.PDFDesigner.category"
            class="fr.malaaria.PDFDesigner.wizards.SampleNewWizard"
            id="fr.malaaria.PDFDesigner.wizards.SampleNewWizard">
      </wizard>
   </extension>
